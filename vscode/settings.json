{
  "workbench.iconTheme": "material-icon-theme",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "workbench.panel.defaultLocation": "right",
  "C_Cpp.updateChannel": "Insiders",
  // "terminal.integrated.shell.linux": "/bin/zsh",
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "go.gopath": "/home/rush/go",
  "go.goroot": "/usr/local/go",
  "go.formatTool": "goformat",
  "go.autocompleteUnimportedPackages": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "golang.go",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  // "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontFamily": "Fira Code",
  "dart.openDevTools": "flutter",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.startupEditor": "none",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 4,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[java]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "editor.inlineSuggest.enabled": true,
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "explorer.confirmDelete": false,
  "bracketPairColorizer.depreciation-notice": false,
  "haskell.serverExecutablePath": "/home/rush/Desktop/oss/haskell-language-server/dist-newstyle/build/x86_64-linux/ghc-9.2.2/haskell-language-server-1.8.0.0/x/haskell-language-server/build/haskell-language-server/haskell-language-server",

  "haskell.formattingProvider": "stylish-haskell",
  "diffEditor.ignoreTrimWhitespace": false,
  "files.autoSave": "afterDelay",
  "security.workspace.trust.untrustedFiles": "open",
  "[FortranFreeForm]": {
    "editor.defaultFormatter": "fortran-lang.linter-gfortran"
  }
  // "haskell.manageHLS": "GHCup"
}
